{
  "expo": {
    "name": "CardMe",
    "slug": "CardMe",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "cardme",
    "facebookScheme": "fb3931214220290227",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSMotionUsageDescription": "Allow $(PRODUCT_NAME) to access your device motion"
      }
    },
    "android": {
      "softwareKeyboardLayoutMode": "pan",
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.NFC"
      ],
      "package": "io.expo.client"
    },
    "plugins": [
      "expo-sensors",
      "expo-barcode-scanner"
    ],
    "web": {},
    "description": ""
  }
}
